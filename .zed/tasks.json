[
  {
    "label": "DevContainer: Up",
    "command": "bash -c 'devcontainer up --log-level trace --workspace-folder ${ZED_WORKTREE_ROOT}'",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Build",
    "command": "bash -c 'devcontainer build --log-level trace --workspace-folder ${ZED_WORKTREE_ROOT}'",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Exec",
    "command": "bash -c 'devcontainer exec --workspace-folder ${ZED_WORKTREE_ROOT} ${1}'",
    "use_new_terminal": true,
    "allow_concurrent_runs": true,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Run User Commands",
    "command": "bash -c 'devcontainer run-user-commands --workspace-folder ${ZED_WORKTREE_ROOT}'",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Start Development",
    "command": "bash -c 'devcontainer up --log-level trace --workspace-folder ${ZED_WORKTREE_ROOT} && devcontainer run-user-commands --workspace-folder ${ZED_WORKTREE_ROOT}'",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  }
]
